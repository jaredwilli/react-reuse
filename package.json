{
    "name": "react-reuse",
    "version": "1.1.0",
    "main": "./lib/index.js",
    "private": false,
    "engines": {
        "node": ">=7.0.0"
    },
    "author": {
        "name": "Jared Williams",
        "email": "jaredwilli@gmail.com",
        "url": "http://anti-code.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jaredwilli/react-reuse"
    },
    "license": "MIT",
    "files": [
        "lib"
    ],
    "homepage": "http://jaredwilli.github.io/react-reuse",
    "keywords": [
        "components",
        "react",
        "react-component",
        "reusable-components",
        "styleguide",
        "pattern-library",
        "toolkit"
    ],
    "dependencies": {
        "codacy-coverage": "^2.1.1",
        "lodash": "^4.17.5",
        "prop-types": "^15.6.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "styled-components": "^1.4.5"
    },
    "scripts": {
        "prestart": "npm run gen:docs",
        "start": "npm-run-all -p start:docs gen:docs-watch",
        "start:docs": "node scripts/start.js",
        "gen:docs": "node scripts/generateComponentData.js",
        "gen:docs-watch": "npm run gen:docs -- --watch",
        "test": "node scripts/test.js --env=jsdom",
        "test-with-coverage": "jest --coverage && cat ./coverage/lcov.info | codacy-coverage --accountToken=0566e091a8b446479663000a7a14c3d6 --username=jaredwilli --projectName=react-reuse",
        "build:css": "cpx \"./src/components/**/*.css\" ./lib",
        "prebuild:lib": "rimraf lib",
        "build:lib": "npm-run-all --parallel build:commonjs build:css build:copy-files",
        "build:copy-files": "node scripts/copyBuildFiles.js",
        "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore test.js",
        "build:docs": "node scripts/build.js",
        "predeploy": "npm run build:docs",
        "deploy": "gh-pages -d build",
        "prepublish": "npm run build:lib"
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx,mjs}"
        ],
        "coveragePathIgnorePatterns": [
            "src/docs/",
            "lib",
            "config",
            "build",
            "coverage",
            "scripts",
            "src/componentData",
            "index.js",
            "src/utils/",
            "src/components/TextInputStyledComponents",
            ".eslintrc.js"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "mjs",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "modules": "commonjs"
                }
            ],
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "devDependencies": {
        "autoprefixer": "7.1.6",
        "babel-cli": "^6.26.0",
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-preset-react-app": "^3.1.0",
        "babel-runtime": "6.26.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "chokidar": "^1.6.1",
        "cpx": "^1.5.0",
        "cross-env": "^5.1.4",
        "css-loader": "0.28.7",
        "dotenv": "4.0.0",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.13.1",
        "eslint-config-react-app": "^2.0.1",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.39.1",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.4.0",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.5",
        "fs-extra": "3.0.1",
        "gh-pages": "^1.1.0",
        "highlight.js": "^9.10.0",
        "html-webpack-plugin": "2.29.0",
        "jest": "^22.0.4",
        "jest-cli": "^22.0.4",
        "jest-enzyme": "^4.0.1",
        "npm-run-all": "^4.0.2",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "promise": "8.0.1",
        "raf": "3.4.0",
        "react-dev-utils": "^4.2.1",
        "react-docgen": "^2.14.0",
        "react-test-renderer": "^16.2.0",
        "rimraf": "^2.6.2",
        "style-loader": "0.19.0",
        "sw-precache-webpack-plugin": "0.11.4",
        "url-loader": "0.6.2",
        "webpack": "3.8.1",
        "webpack-dev-server": "2.9.4",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3"
    }
}
